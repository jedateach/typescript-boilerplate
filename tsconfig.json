{
    "compilerOptions": {
        "target": "ES5",
        "module": "ESNext",
        "moduleResolution": "node",
        "outDir": "dist/esnext",
        "baseUrl": "./",
        "paths": {
            "mylib": ["src/mylib"]
        },
        "inlineSourceMap": true,
        "inlineSources": true
    },
    "typedocOptions": {
        "inputFiles": "src",
        "mode": "file",
        "out": "docs/",
        "name": "mylib",
        "theme": "minimal"
    }
}